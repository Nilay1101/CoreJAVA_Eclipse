package MeConsOverload;

class Overload{
	int a;
	int b;
	
	Overload(){
		a=10;
		b=11;
		System.out.println("a & b : " +a +b);
	}
	
	Overload(int x, int y){
		a=x;
		b=y;
		System.out.println("a & b : " +a +b);
	}
}

public class MeConsOverload {
	public static void main(String args[]){
		Overload obj1 = new Overload();
		Overload obj2 = new Overload(0,1);
		
	}

}
