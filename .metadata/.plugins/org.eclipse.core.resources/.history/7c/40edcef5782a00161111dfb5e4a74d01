package StackClass;

class Stack{
	char stk[]=new char[10];
	int top;
	char item;
	
	Stack()
	{
		top=0;
	}
	
	void push( char a){
		item=a;
		if(top==9)
			System.out.println("Sorry the stack is full!!!!!");
		else{
			stk[top]=item;
		    top++;
	}
	}
	
	void pop(){
		if(top==-1)
			System.out.println("Sorry the stack is empty!!!!!");
		else{
			stk[top]='\0';
			top--;
	}
	}
	
	void display()
	{
		for(int i=0;i<10;i++)
		{
			System.out.println(stk[i]);
		}
	}
	
}

public class StackClass {
	public static void main(String args[])
	{char a='N';
		Stack obj=new Stack();
		for(int i=0;i<10;i++){
			obj.push('N');
			a++;
			}
		obj.display();
		for(int i=0;i<10;i++){
			obj.pop();
			}
		}

}
