package SuperInh;

class Master{
	int a;
	int b;
	 Master(){
		 a=10;
		 System.out.println("Master Constructor initializes Master a to : " +a);
		 
	 }
	 
	 Master(int x){
		 a=x;
		 System.out.println("Parameterized Master Constructor initializes Master a to : " +a);
	 }
}

class Slave extends Master{
	int a;
	Slave(){
		b=11;
		System.out.println("Slave Constructor initializes Master b to : " +b);
		
	}
	
}

public class SuperInh {
	public static void main(String args[]){
		Master obj = new Master();
		Master obj2 = new Master(12);
		Slave obj1 = new Slave();
	}

}
